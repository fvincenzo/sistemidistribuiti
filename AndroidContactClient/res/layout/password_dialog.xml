<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget0"
android:layout_width="260px"
android:layout_height="160px"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/widget29"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="To confirm insert OLD password"
android:layout_x="17px"
android:layout_y="7px"
>
</TextView>
<EditText
android:id="@+id/pwd_password"
android:layout_width="218px"
android:layout_height="wrap_content"
android:textSize="18sp"
android:password="true"
android:layout_x="20px"
android:layout_y="31px"
>
</EditText>
<Button
android:id="@+id/pwd_ok"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="OK"
android:textSize="18sp"
android:textStyle="bold"
android:textColor="#ff666666"
android:layout_x="20px"
android:layout_y="90px"
>
</Button>
<Button
android:id="@+id/pwd_cancel"
android:layout_width="104px"
android:layout_height="wrap_content"
android:text="CANCEL"
android:textSize="18sp"
android:textStyle="bold"
android:textColor="#ff666666"
android:layout_x="135px"
android:layout_y="90px"
>
</Button>
</AbsoluteLayout>
