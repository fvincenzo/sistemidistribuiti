<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget30"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/widget33"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Insert your username"
android:layout_x="30px"
android:layout_y="12px"
>
</TextView>
<EditText
android:id="@+id/Username"
android:layout_width="260px"
android:layout_height="45px"
android:textSize="18sp"
android:layout_x="30px"
android:layout_y="32px"
>
</EditText>
<TextView
android:id="@+id/widget34"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Insert your password"
android:layout_x="30px"
android:layout_y="92px"
>
</TextView>
<EditText
android:id="@+id/Password"
android:layout_width="260px"
android:layout_height="45px"
android:layout_marginLeft="50px"
android:textSize="18sp"
android:password="true"
android:layout_x="30px"
android:layout_y="112px"
>
</EditText>
<TextView
android:id="@+id/widget33"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Insert something else"
android:layout_x="30px"
android:layout_y="172px"
>
</TextView>
<EditText
android:id="@+id/Something"
android:layout_width="260px"
android:layout_height="45px"
android:textSize="18sp"
android:layout_x="30px"
android:layout_y="192px"
>
</EditText>


<Button
android:id="@+id/Register"
android:layout_width="96px"
android:layout_height="wrap_content"
android:text="Register"
android:textSize="18sp"
android:textStyle="bold"
android:textColor="#ff666666"
android:layout_x="100px"
android:layout_y="362px"
>
</Button>
</AbsoluteLayout>
